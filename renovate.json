{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "apankowski"
  ],
  "configMigration": true,
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "groupName": "non-major"
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "groupName": "github-actions"
    }
  ],
  "prConcurrentLimit": 2
}
